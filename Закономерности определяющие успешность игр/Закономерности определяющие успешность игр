   "source": [
    "# сгруппируем по платформе и отсортируем в порядке возрвстания по общим продажам\n",
    "data_platform = data.groupby('platform')['total_sales'].sum().sort_values(ascending=False)\n",
    "# переведем в dataFrame\n",
    "data_platform = data_platform.to_frame().reset_index()\n",
    "# расчитаем долю продаж платформы от общих продаж\n",
    "data_platform['percent'] = data_platform['total_sales']/data_platform['total_sales'].sum()\n",
    "# добавим столбец с кумулятивной суммой долей\n",
    "data_platform['cum_percent'] = data_platform['percent'].cumsum()\n",
    "data_platform"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "df30aad8",
   "metadata": {},
   "outputs": [],
   "source": [
    "# присвоим категорию ABC для платформ\n",
    "data_platform['category'] = data_platform['cum_percent'].apply(lambda x:\"A\" if x<=0.8 else \"B\" if x<0.95 else \"C\") "
   ]
  },
