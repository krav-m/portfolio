    "# сгруппируем общие продажи платформ категории А по годам\n",
    "data_years = data[data['category'] == \"A\"].pivot_table(index=['platform','year_of_release'], values = 'total_sales', aggfunc='sum')\n",
    "data_years = data_years.reset_index()\n",
    "data_years"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "id": "abf9ca8b",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "['DS', 'GBA', 'PS', 'PS2', 'PS3', 'PS4', 'PSP', 'Wii', 'X360']"
      ]
